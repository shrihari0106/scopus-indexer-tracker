<% var results = data["abstracts-retrieval-response"]["coredata"] %>
<% var creators = results["dc:creator"]["author"] %>
<% var title = results["dc:title"] %>
<% var description = results["dc:description"] %>
  <%if (typeof(description) == 'undefined') { %>
    <% description = "No Description Provided" %>
    <%}%>  
<% var cited = results["citedby-count"] %>
<% var publisher = results["prism:publicationName"] %>
<% var type = results["subtypeDescription"] %>
<% var date = results["prism:coverDate"] %>
<% var auinit = [] %>
<% var augiven = [] %>
<% var ausur = [] %>
<% var auid = [] %>
<% for(var i=0;i<creators.length;i++) {%>
    <% auinit.push(creators[i]['ce:initials']) %>
    <% augiven.push(creators[i]['ce:given-name']) %>   
    <% ausur.push(creators[i]['ce:surname']) %> 
    <% auid.push(creators[i]['@auid']) %> 
<%}%>
